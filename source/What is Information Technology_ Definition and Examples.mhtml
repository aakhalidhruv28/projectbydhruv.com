  header .header-search-toggle-hide, header .header-search-toggle-show { background-size: 30px; background-image: url("https://cdn-ttgtmedia-com.cdn.ampproject.org/ii/AW/s/cdn.ttgtmedia.com/rms/ux/amp/img/icon_search@2x.png"); }
}

.eyebrow { display: block; color: rgb(102, 102, 102); font-size: 12px; padding-bottom: 15px; text-transform: uppercase; }

.contributors { font-size: 12px; border-top: 1px solid rgb(227, 227, 227); padding-top: 10px; margin-top: 10px; }

.contributors span { color: rgb(102, 102, 102); }

.contributors a { color: teal; }

.continue-reading { background: url("https://cdn-ttgtmedia-com.cdn.ampproject.org/ii/AW/s/cdn.ttgtmedia.com/rms/ux/amp/img/border_diagonal.png") 0px top repeat-x; padding-top: 20px; margin-top: 20px; }

.continue-reading ul { list-style: none; }

.continue-reading ul li { font-size: 15px; margin-bottom: 15px; padding-left: 40px; position: relative; }

.continue-reading ul li::before { color: teal; content: ""; font-size: 108px; line-height: 16px; position: absolute; left: 5px; top: 0px; }

.continue-reading ul li:last-child { margin-bottom: 0px; }

.continue-reading ul li a { color: teal; }

.continue-reading ul li span { display: block; font-size: 12px; color: rgb(102, 102, 102); }

.related-terms { background: url("https://cdn-ttgtmedia-com.cdn.ampproject.org/ii/AW/s/cdn.ttgtmedia.com/rms/ux/amp/img/border_diagonal.png") 0px top repeat-x; padding-top: 20px; margin-top: 20px; }

.related-terms ul { list-style: none; }

.related-terms ul li { border-bottom: 1px solid rgb(227, 227, 227); padding-bottom: 15px; font-size: 16px; margin-bottom: 15px; }

.related-terms ul li:first-child { padding-top: 5px; }

.related-terms ul li:last-child { margin-bottom: 0px; border: 0px; }

.related-terms ul li a { color: rgb(0, 124, 173); }

.related-terms ul li span { display: block; font-size: 12px; color: rgb(102, 102, 102); }

.btt-thumbnailContainer { position: relative; display: block; margin: 0px auto 20px; box-shadow: rgba(0, 0, 0, 0.29) 0px 0px 3px; }

.btt-thumbnailContainer .btt-thumbnailTitle { position: absolute; background: rgba(0, 0, 0, 0.8); z-index: 1; color: rgb(255, 255, 255); font-size: 14px; line-height: 20px; padding: 5px 10px; width: 100%; box-sizing: border-box; }

.btt-thumbnailContainer .btt-video-duration { position: absolute; bottom: 10px; right: 10px; background: rgb(0, 0, 0); color: rgb(255, 255, 255); padding: 2px; font-size: 12px; }

.btt-thumbnailImgContainer::before { content: ""; position: absolute; top: calc(50% - 13px); left: calc(50% - 35px); width: 70px; height: 50px; border-radius: 3px; background: rgba(98, 98, 98, 0.85); z-index: 1; }

.btt-thumbnailImgContainer::after { content: ""; position: absolute; top: 50%; left: calc(50% - 8px); width: 0px; height: 0px; border-top: 12px solid transparent; border-bottom: 12px solid transparent; border-left: 22px solid rgb(255, 255, 255); opacity: 0.7; z-index: 2; }

.btt-thumbnailImgContainer .btt-videoBtThumbnail { width: 100%; display: block; }
